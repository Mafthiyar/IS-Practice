import java.util.*;

class solution {
    public static void main(String[] args) {
        String a = "Mafthiyar";
        int freq[] = new int[256];

        for (char c : a.toCharArray()) {
            freq[c]++;
        }

        for (int i = 0; i < 256; i++) {
            if (freq[i] > 0) {
                System.out.println((char) i + " : " + freq[i]);
            }
        }
    }
}
