class MaxDifference {
    public static void main(String[] args) {
        int[] arr= {2,3,6};

        int min = arr[0];
        int max = arr[1] - arr[0];

        for(int i=1; i<arr.length; i++){
            max = Math.max(max,arr[i] - min);
            min = Math.min(min,arr[i]);
        }
        System.out.println(max);
    }
}
